terraform {
  required_providers {
    aws = {
      source  = "hashicorp/aws"
      version = "~>5.0"
    }
  }
}
provider "aws" {
  region = "ap-south-1"

}
resource "aws_instance" "bala" {
  ami            = "ami-0da59f1af71ea4ad2"
  instance_type = "t2.micro"
  subnet_id      = "default"  ### this default own creation
  key_name       = "777"

  tags = {
    name = "bala"
  }
}
